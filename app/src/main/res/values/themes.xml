<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.AirVetInterviewTest" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">#0066FF</item>
        <item name="colorPrimaryVariant">#0066FF</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
    </style>

    <style name="Theme.AirVetInterviewTest.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AirVetInterviewTest.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.AirVetInterviewTest.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>